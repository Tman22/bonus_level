<h1>Edit <%= @article.title %></h1>
<%= form_for @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>

  <%= f.label :article_image %>
  <%= f.text_field :article_image %>
  <br>

  <%= f.label :body %>
  <%= f.text_area :body  %>
  <br>

  <%= f.label :tag_list %>
  <%= f.text_field :tag_list, value: "" %>

  <%= f.submit "Complete" %>

<% end  %>
